{
  "name": "halloverden/symfony-http-exceptions",
  "description": "Symfony HTTP Expcetions",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Roar Furunes",
      "email": "tom.roar@halloverden.no"
    },
    {
      "name": "Erich Json Fosse",
      "email": "erich@halloverden.no"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "ext-json": "*",
    "php": ">=8.1",
    "symfony/http-kernel": "^5.4|^6.4|^7.0"
  },
  "require-dev": {
    "symfony/validator": "^7.0"
  },
  "suggest": {
    "symfony/validator": "for ValidationException"
  },
  "autoload": {
    "psr-4": {
      "HalloVerden\\HttpExceptions\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  }
}
